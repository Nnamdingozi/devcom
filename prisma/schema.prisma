generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")  // Make sure this exists in .env
}




model User {
  id        Int      @id @default(autoincrement())
  email     String   @unique
  password  String
  username      String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
